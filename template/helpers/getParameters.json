{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "rgLocation": {
            "type": "string"
        },
        "rgNames": {
            "type": "array",
            "metadata": {
                "description": "Array of spoke objects. Make sure to add subnet called AzureFirewallSubnet if you intend to deploy an MS Firewall"
            }
        },
        "tagValues": {
            "type": "object"
        }
    },
    "variables": {},
    "resources": [],
    "outputs": {
        "rgLocation": {
            "value": "[parameters('rgLocation')]",
            "type": "array"
        },
        "rgNames": {
            "value": "[parameters('rgNames')]",
            "type": "array"
        },
        "tagValues": {
            "value": "[parameters('tagValues')]",
            "type": "object"
        }
    }
}